{
    "compilerOptions": {
        "target": "ES2021",
        "module": "CommonJS",
        "lib": [
            "ES2021"
        ],
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node"
